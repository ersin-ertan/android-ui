	android:layout_width = "match_parent"
	tools:context = "com.nullcognition.effectiveandroidui.ui.activity.MainActivity"
	tools:ignore = "MergeRootFrame"
	xmlns:android = "http://schemas.android.com/apk/res/android"
	xmlns:tools = "http://schemas.android.com/tools"
	>

	<!-- TvShowCatalogFragment -->

	<fragment
		android:id = "@+id/f_tv_show_catalog"
		android:layout_height = "match_parent"
		android:layout_width = "match_parent"
		class = "com.nullcognition.effectiveandroidui.ui.fragment.TvShowCatalogFragment"
		tools:context = "com.nullcognition.effectiveandroidui.ui.activity.MainActivity"
		/>

	<!-- TvShowDraggableFragment -->

	<fragment
		android:id = "@+id/f_tv_show_draggable"
		android:layout_height = "match_parent"
		android:layout_width = "match_parent"
		class = "com.nullcognition.effectiveandroidui.ui.fragment.TvShowDraggableFragment"
		tools:context = "com.nullcognition.effectiveandroidui.ui.activity.MainActivity"
		/>

</FrameLayout>
